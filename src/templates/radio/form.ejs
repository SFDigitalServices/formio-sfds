<div class="form-group field-s08">
  <fieldset>
    <legend>
      {{ctx.t(ctx.component.label)}}
    </legend>
    <div class="field-wrapper">
      {% ctx.values.forEach(function(item) { %}
        <label>
          <{{ctx.input.type}}
            ref="input"
            {% for (var attr in ctx.input.attr) { %}
            {{attr}}="{{ctx.input.attr[attr]}}"
            {% } %}
            value="{{item.value}}"
            {% if (ctx.value && (ctx.value === item.value || (typeof ctx.value === 'object' && ctx.value.hasOwnProperty(item.value) && ctx.value[item.value]))) { %}
              checked=true
            {% } %}
            {% if (item.disabled) { %}
              disabled=true
            {% } %}
            id="{{ctx.id}}{{ctx.row}}-{{item.value}}"
          >
          <span class="inline-label">{{ctx.t(item.label)}}</span>
        </label>
      {% }) %}
    </div>
    <div class="help-block with-errors"></div>
  </fieldset>
</div>
