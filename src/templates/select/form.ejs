<select
  ref="{{ ctx.input.ref || 'selectContainer' }}"
  {{ ctx.requiredAttributes() }}
  id="{{ ctx.inputId() }}"
  {% if (ctx.input.multiple) { %}
    multiple
  {% } %}
  {% for (var attr in ctx.input.attr) { %}
    {{ attr }}="{{ ctx.input.attr[attr] }}"
  {% } %}
>{{ctx.selectOptions}}</select>
