<nav aria-label="navigation" id="{{ ctx.wizardKey }}-header">
  <ul class="list-style-none">
    {% ctx.panels.forEach(function(panel, index) { %}
      {% var active = ctx.currentPage === index; %}
      <li class="border-bottom-1 border-grey-3" {% if (active) { %} aria-current="page"{% } %}>
        <span class="d-block pr-3 py-2 cursor-pointer{{ active ? ' fw-medium' : '' }}" ref="{{ ctx.wizardKey }}-link">
          {{ ctx.t(panel.title) }}
        </span>
      </li>
    {% }) %}
  </ul>
</nav>
