<nav aria-label="navigation" id="{{ ctx.wizardKey }}-header">
  <ul class="list-style-none mb-3">
    {% ctx.panels.forEach(function(panel, index) { %}
    {% var completed = index < ctx.currentPage; %}
    {% var active = index === ctx.currentPage; %}
    <li class="d-flex flex-items-center" {% if (active) { %} aria-current="page" {% } %}>
      {% if (active) { %}
      <span data-icon="check" class="pr-1"></span>
      {% } %}
      <span class="flex-auto py-1 cursor-pointer{% if (active || completed) { %} fw-medium{% } %}">
        {{ ctx.t(panel.title) }}
      </span>
    </li>
    {% }) %}
  </ul>
</nav>