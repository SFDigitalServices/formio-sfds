{
  "name": "formio-sfds",
  "version": "1.0.0",
  "description": "form.io templates for the SF Design System",
  "module": "src/index.js",
  "main": "dist/formio-sfds.cjs.js",
  "browser": "dist/formio-sfds.umd.js",
  "style": "dist/formio-sfds.css",
  "scripts": {
    "build": "npm-run-all -p 'build-*'",
    "build-css": "sass -I node_modules --style compressed src/scss/index.scss:$npm_package_style",
    "build-js": "rollup -c",
    "lint": "npm-run-all -p 'lint-*'",
    "lint-css": "stylelint src/scss/**/*.scss",
    "lint-js": "standard src",
    "prebuild": "rm -rf dist",
    "watch": "npm-run-all -p 'watch-*'",
    "watch-css": "npm run build-css -- --watch",
    "watch-js": "rollup -c --watch"
  },
  "browserslist": [
    "> 1%",
    "ie 11"
  ],
  "repository": "SFDigitalServices/formio-sfds",
  "keywords": [
    "formio",
    "sf-design-system"
  ],
  "author": "San Francisco Digital Services",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "sass": "^1.26.3",
    "sf-design-system": "^1.2.6",
    "standard": "^14.3.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.15.0"
  },
  "dependencies": {
    "formiojs": "^4.9.0-rc.10"
  }
}
