{
  "name": "formio-sfds",
  "version": "1.2.0",
  "description": "form.io templates for the SF Design System",
  "module": "src/index.js",
  "main": "dist/formio-sfds.cjs.js",
  "browser": "dist/formio-sfds.umd.js",
  "style": "dist/formio-sfds.css",
  "scripts": {
    "build": "run-p 'build-*'",
    "build-css": "postcss src/scss/index.scss -o $npm_package_style",
    "build-js": "rollup -c",
    "lint": "run-p 'lint-*'",
    "lint-css": "stylelint src/scss/**/*.scss",
    "lint-js": "standard src",
    "test": "echo 'WARNING: no tests!'",
    "watch": "run-p 'watch-*'",
    "watch-css": "watchy -w 'src/scss/**/*.scss' -- npm run build-css",
    "watch-js": "rollup -c --watch"
  },
  "browserslist": [
    "> 1%",
    "ie 11"
  ],
  "repository": "SFDigitalServices/formio-sfds",
  "keywords": [
    "formio",
    "sf-design-system"
  ],
  "author": "San Francisco Digital Services",
  "license": "MIT",
  "dependencies": {
    "formiojs": "^4.9.0-rc.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@csstools/postcss-sass": "^4.0.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "postcss-scss": "^2.0.0",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-jst": "^1.2.0",
    "rollup-plugin-svgo": "^1.1.0",
    "sass": "^1.26.3",
    "selector-observer": "^2.1.6",
    "sf-design-system": "^1.2.6",
    "standard": "^14.3.3",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.15.0",
    "watchy": "^0.9.7"
  }
}
