{
  "rewrites": [
    {
      "source": "/",
      "destination": "/dist/index.html"
    },
    {
      "source": "/:view.html",
      "destination": "/dist/:view.html"
    },
    {
      "source": "/examples/:id",
      "destination": "/dist/examples/:id.html"
    },
    {
      "source": "/:view",
      "destination": "/dist/:view.html"
    }
  ]
}
