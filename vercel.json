{
  "rewrites": [
    {
      "source": "/",
      "destination": "/dist/index.html"
    },
    {
      "source": "/:view",
      "destination": "/dist/:view.html"
    },
    {
      "source": "/examples/:example",
      "destination": "/dist/examples/:example.html"
    }
  ],
  "build": {
    "env": {
      "I18N_SERVICE_URL": "https://i18n-microservice-js-git-phrase-endpoint.sfds.vercel.app"
    }
  }
}
