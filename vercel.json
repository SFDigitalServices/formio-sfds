{
  "routes": [
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "src": "/([-\\w]+)",
      "dest": "/dist/$1.html"
    },
    {
      "src": "/examples/([-\\w]+)",
      "dest": "/dist/examples/$1.html"
    }
  ],
  "build": {
    "env": {
      "I18N_SERVICE_URL": "https://translate.sf.gov"
    }
  }
}
