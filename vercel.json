{
  "routes": [
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(\\w+).html",
      "dest": "/dist/$1.html"
    },
    {
      "src": "/(\\w+)",
      "dest": "/dist/$1.html"
    },
    {
      "src": "/examples/(\\w+)",
      "dest": "/dist/examples/$1.html"
    }
  ],
  "build": {
    "env": {
      "I18N_SERVICE_URL": "https://i18n-microservice-js-git-phrase-endpoint.sfds.vercel.app"
    }
  }
}
